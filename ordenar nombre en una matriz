#include "stdafx.h"
#include <iostream>
#include "stdlib.h"
#include "conio.h"
#include <string>

#define MAX 10

using namespace std;
//Struct::

string Matriz_Nombres[MAX][MAX];
int FIL, COL;

//Prototipos de Funciones::
void Llenar(string M[MAX][MAX], int F, int C);
void Ordenar_Nombres(string M[MAX][MAX], int F, int C);
void Mostrar(string M[MAX][MAX], int F, int C);

// Main:: Principal
void main()
{
	cout << "\n numero de Filas: ";
	cin >> FIL;
	cout << "\n numero de Columnas: ";
	cin >> COL;
	cout << endl;

	Llenar(Matriz_Nombres, FIL, COL);
	cout << "\n\n";
	cout << "Nombres ordenados" << endl;
	Ordenar_Nombres(Matriz_Nombres, FIL, COL);
	Mostrar(Matriz_Nombres, FIL, COL);


	_getch();
}

void Llenar(string M[MAX][MAX], int F, int C)
{
	cin.ignore();
	for (int i = 1; i <= F; i++) 
	{
		for (int j = 1; j <= C; j++)
		{
			cout << "\n Nombre [" << i << "][" << j << "]: ";
			getline(cin, M[i][j]);
		}
	}
}

void Ordenar_Nombres(string M[MAX][MAX], int F, int C)
{
	string AUX;
	for (int i = 1; i <= F; i++)
	{
		for (int j = 1; j <= C; j++)
		{
			for (int u = 1; u <= F; u++) 
			{
				for (int v = 1; v <= C; v++) 
				{
					if (M[i][j]<M[u][v]) 
					{
						AUX = M[i][j];
						M[i][j] = M[u][v];
						M[u][v] = AUX;
					}
				}
			}
		}
	}
}

void Mostrar(string M[MAX][MAX], int F, int C)
{
	for (int i = 1; i <= F; i++)
	{
		for (int j = 1; j <= C; j++)
		{
			cout << "Nombre [" << i << "][" << j << "]: ";
			cout << M[i][j];
			cout << endl;
		}

	}
}
